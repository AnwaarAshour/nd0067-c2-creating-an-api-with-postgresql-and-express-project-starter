{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "migrate": "db-migrate --env test up && db-migrate up",
    "test": "ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "lint": "eslint 'src/*.ts'",
    "linth": "eslint 'src/handlers/*.ts'",
    "lintm": "eslint 'src/models/*.ts'",
    "prettier": "prettier --config .prettierrc 'src/*.ts' --write",
    "prettierh": "prettier --config .prettierrc 'src/handlers/*.ts' --write",
    "prettierm": "prettier --config .prettierrc 'src/models/*.ts' --write"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.1",
    "psql": "^0.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.17",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.8.1",
    "tsc-watch": "^4.2.9"
  }
}
